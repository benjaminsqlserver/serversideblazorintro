{
  "access": "Everybody",
  "components": [
    {
      "components": [
        {
          "name": "pageTitle",
          "size": "H1",
          "text": "Login",
          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "allowRegister": false,
                  "allowResetPassword": false,
                  "events": {
                    "login": [],
                    "register": [
                      {
                        "path": "register-application-user",
                        "type": "popup"
                      }
                    ]
                  },
                  "name": "login0",
                  "type": "login"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "code": "var error = System.Web.HttpUtility.ParseQueryString(new Uri(UriHelper.ToAbsoluteUri(UriHelper.Uri).ToString()).Query).Get(\"error\");",
        "type": "execute"
      },
      {
        "condition": "!string.IsNullOrEmpty(error)",
        "detail": "${error}",
        "severity": "error",
        "summary": "Error",
        "type": "notify"
      }
    ]
  },
  "layout": "Login",
  "name": "Login"
}